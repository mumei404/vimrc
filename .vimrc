syntax enable
set background=dark
colorscheme solarized

set number



set autoindent
set smartindent
set tabstop=4
set shiftwidth=4
"set rtp+=~/vimPlugins/tabnine-vim
inoremap <?<Enter> <?php  ?><Left><Left><Left>
inoremap <%<Enter> <%  %><Left><Left><Left>
inoremap <%=<Enter> <%=  %><Left><Left><Left>
inoremap {<Enter> {}<Left><CR><C-d><Esc><S-o>
inoremap (<Enter> ()<Left><CR><C-d><Esc><S-o>
inoremap [<Enter> []<Left><CR><C-d><Esc><S-o>
inoremap { {}<Left>
inoremap ( ()<Left>
inoremap [ []<Left>
inoremap < <><Left>
inoremap " ""<Left>
inoremap ' ''<Left>

